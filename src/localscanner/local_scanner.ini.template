[socket]
# 시스템에서 사용할 인터페이스
# wlx0013ef70600e, wlp4s0, wlan0
interface = wlan1 
timeout = 5

[log]
# Level    Numeric value
# CRITICAL       50
# ERROR          40
# WARNING        30
# INFO           20
# DEBUG          10
# NOTSET          0
level = DEBUG
file = ./scanner.log

[handle]
interval = 60
device_id = pi
scan_db = local_scan.sqlite
